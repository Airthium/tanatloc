{
  "source": {
    "include": ["."],
    "exclude": [
      "app",
      "config",
      "coverage",
      "dist-server",
      "dist",
      "doc",
      "node_modules",
      ".next",
      "out",
      "modules/three-to-glb/lib/three"
    ]
  },
  "opts": {
    "destination": "./doc",
    "recurse": true,
    "readme": "README.md",
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "static_dir": ["./public"],
      "title": "<div style=\"display: flex; justify-content: space-around; align-items: center; padding: 16px;\"><img src='./public/images/icon.png' width=\"64px\" height=\"64px\"/> Tanatloc</div>",
      "footer": "Tanatloc - See the world the way it really is! - Copyright Â© 2021"
    }
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "systemLogo": "./public/images/icon.png",
    "favicon": "./public/images/icon.png"
  }
}
