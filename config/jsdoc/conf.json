{
  "source": {
    "include": ["."],
    "exclude": [
      "app",
      "coverage",
      "dist-server",
      "dist",
      "doc",
      "node_modules",
      ".next",
      "out"
    ]
  },
  "opts": {
    "destination": "./doc",
    "recurse": true,
    "readme": "README.md",
    "template": "./node_modules/foodoc/template"
  },
  "templates": {
    "systemName": "Tanatloc documentation",
    "systemSummary": "See the world the way it really is!",
    "systemLogo": "./public/images/icon.png",
    "copyright": "Tanatloc Copyright Â© 2021",
    "favicon": "./public/images/icon.png"
  }
}
