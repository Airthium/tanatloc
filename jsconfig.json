            "@/config": ["config"],
            "@/config/*": ["config/*"],
            "@/models": ["models"],
            "@/models/*": ["models/*"],
            "@/templates": ["templates"],
            "@/templates/*": ["templates/*"],
            "@/plugin": ["plugin"],
            "@/plugin/*": ["plugin/*"],
            "@/api": ["src/api"],
            "@/api/*": ["src/api/*"],
