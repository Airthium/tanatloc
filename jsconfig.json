            "@/pages": ["src/pages"],
            "@/pages/*": ["src/pages/*"],
            "@/route": ["src/route"],
            "@/route/*": ["src/route/*"],
            "@/services": ["src/services"],
            "@/services/*": ["src/services/*"],
            "@/store": ["src/store"],
