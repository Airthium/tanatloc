{
  "name": "tanatloc",
  "description": "Tanatloc",
  "version": "1.0.0-alpha.4",
  "repository": "git@github.com:Airthium/tanatloc-ssr.git",
  "license": "UNLICENSED",
  "private": true,
  "homepage": "./",
  "author": {
    "name": "Airthium",
    "url": "https://github.com/orgs/Airthium/people"
  },
  "main": "app/background.js",
  "browser": {
    "fs": false,
    "path": false
  },
  "scripts": {
    "build": "NEXT_PUBLIC_SOURCE_BRANCH=`git rev-parse --abbrev-ref HEAD` NEXT_PUBLIC_SOURCE_COMMIT=`git rev-parse --short HEAD` next build",
    "depcheck": "depcheck --config=config/depcheck/.depcheckrc.json",
    "dev": "NODE_OPTIONS=\"--max_old_space_size=8192\" NEXT_PUBLIC_SOURCE_BRANCH=`git rev-parse --abbrev-ref HEAD` NEXT_PUBLIC_SOURCE_COMMIT=`git rev-parse --short HEAD` next",
    "doc": "jsdoc -c config/jsdoc/conf.js",
    "tsdoc": "typedoc --options config/typedoc/config.json",
    "docker": "docker build . -t tanatloc/tanatloc:test",
    "electron-build": "yarn postinstall && yarn serverbuild && nextron build && electron-builder install-app-deps",
    "electron-dev": "yarn postinstall && yarn serverbuild && nextron",
    "export": "yarn build && next export",
    "postinstall": "yarn postinstallconfig && yarn postinstallinstall && yarn postinstalldatabase && yarn postinstallrun",
    "postinstallconfig": "swc config --out-dir dist-install/config",
    "postinstallinstall": "swc install --out-dir dist-install/install",
    "postinstalldatabase": "swc src/database --out-dir dist-install/src",
    "postinstallrun": "node dist-install/install",
    "prettier": "prettier -l '**/*.{js,jsx,ts,tsx,json,md,css}'",
    "prettier:write": "prettier --write '**/*.{js,jsx,ts,tsx,json,md,css}'",
    "serverbuild": "yarn serverbuildconfig && yarn serverbuildmodels && yarn serverbuildmodules && yarn serverbuildplugins && yarn serverbuildserver && yarn serverbuildauth && yarn serverbuilddatabase && yarn serverbuildlib && yarn serverbuildroute && yarn serverbuildservices && yarn serverbuildtemplates",
    "serverbuildconfig": "swc config --out-dir dist-server/config",
    "serverbuildmodels": "swc models --out-dir dist-server/models",
    "serverbuildmodules": "swc modules --out-dir dist-server/modules",
    "serverbuildplugins": "swc plugins --out-dir dist-server/plugins",
    "serverbuildserver": "swc server --out-dir dist-server/server",
    "serverbuildauth": "swc src/auth --out-dir dist-server/src",
    "serverbuilddatabase": "swc src/database --out-dir dist-server/src",
    "serverbuildlib": "swc src/lib --out-dir dist-server/src",
    "serverbuildroute": "swc src/route --out-dir dist-server/src",
    "serverbuildservices": "swc src/services --out-dir dist-server/src",
    "serverbuildtemplates": "swc templates --out-dir dist-server/templates",
    "start": "NODE_OPTIONS=\"--max_old_space_size=8192\" next start",
    "test": "jest --config config/jest/jest.config.js --coverage --silent",
    "testwatch": "jest --config config/jest/jest.config.js --watch",
    "preteste2e": "DB_DATABASE=tanatloc_test STORAGE_PATH=/tmp/tanatloc_test yarn postinstall",
    "teste2e": "jest --config config/jest/e2e/jest.config.js --coverage --silent --runInBand",
    "teste2ewatch": "jest --config config/jest/e2e/jest.config.js --watch --runInBand",
    "workflow": ".github/workflows/node.local.sh"
  },
  "dependencies": {
    "electron-serve": "1.1.0",
    "electron-store": "8.0.1"
  },
  "devDependencies": {
    "@airthium/air-jsdoc-template": "1.0.4",
    "@ant-design/icons": "4.7.0",
    "@hapi/iron": "6.0.0",
    "@reduxjs/toolkit": "1.6.2",
    "@sentry/browser": "6.15.0",
    "@sentry/node": "6.15.0",
    "@swc/cli": "0.1.52",
    "@swc/core": "1.2.115",
    "@swc/jest": "0.2.11",
    "@testing-library/react": "12.1.2",
    "@types/jest": "27.0.3",
    "antd": "4.17.2",
    "archiver": "5.3.0",
    "better-react-mathjax": "1.0.3",
    "cookie": "0.4.1",
    "cors": "2.8.5",
    "deep-object-diff": "1.1.0",
    "depcheck": "1.4.2",
    "ejs": "3.1.6",
    "electron": "16.0.3",
    "electron-builder": "22.14.5",
    "eslint": "8.3.0",
    "eslint-config-next": "12.0.4",
    "express": "4.17.1",
    "http-errors": "1.8.1",
    "ignore-loader": "0.1.2",
    "is-docker": "3.0.0",
    "is-electron": "2.2.1",
    "jest": "27.4.2",
    "jest-canvas-mock": "2.3.1",
    "jsdoc": "3.6.7",
    "lodash.merge": "4.6.2",
    "mailersend": "1.2.0",
    "next": "12.0.4",
    "next-compose-plugins": "2.2.1",
    "next-plugin-antd-less": "1.5.2",
    "nextron": "7.1.0",
    "node-fetch": "3.1.0",
    "passport": "0.5.0",
    "passport-local": "1.0.0",
    "pg": "8.7.1",
    "pg-native": "./modules/pg-native",
    "prettier": "2.5.0",
    "prism-themes": "1.9.0",
    "prismjs": "1.25.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "react-simple-code-editor": "0.11.0",
    "recharts": "2.1.6",
    "redux": "4.1.2",
    "redux-devtools-extension": "2.13.9",
    "redux-persist": "6.0.0",
    "set-interval-async": "2.0.3",
    "swr": "1.1.0",
    "tar": "6.1.11",
    "three": "0.135.0",
    "three-to-glb": "./modules/three-to-glb/",
    "typedoc": "0.22.10",
    "typescript": "4.5.2",
    "url-join": "4.0.1",
    "uuid": "8.3.2"
  }
}
