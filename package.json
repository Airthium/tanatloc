{
  "name": "tanatloc",
  "version": "1.3.0",
  "description": "Tanatloc",
  "homepage": "./",
  "repository": "git@github.com:Airthium/tanatloc.git",
  "license": "AGPL-3.0-or-later",
  "author": {
    "name": "Airthium",
    "url": "https://github.com/orgs/Airthium/people"
  },
  "type": "module",
  "scripts": {
    "depcheck": "node config/depcheck/depcheckrc.js",
    "prettier": "prettier -l '**/*.{js,jsx,ts,tsx,json,md,css}'",
    "prettier:write": "prettier --write '**/*.{js,jsx,ts,tsx,json,md,css}'",
    "doc": "typedoc --options config/typedoc/config.json",
    "testwatch": "jest --config config/jest/jest.config.js --watch",
    "test": "jest --config config/jest/jest.config.js --coverage --silent --runInBand",
    "sonar": ".github/workflows/sonar.local.sh",
    "prestart": "yarn prestart:config && yarn prestart:database && yarn prestart:init && yarn prestart:package && yarn prestart:install && yarn prestart:run",
    "prestart:config": "swc config/db.ts --out-dir dist-install/config && swc config/storage.ts --out-dir dist-install/config",
    "prestart:database": "swc src/database/index.ts --out-dir dist-install/src",
    "prestart:init": "swc src/server/init/database.ts --out-dir dist-install/src",
    "prestart:package": "swc package.json --out-dir dist-install --copy-files",
    "prestart:install": "swc install --out-dir dist-install/install",
    "prestart:run": "node --experimental-specifier-resolution=node dist-install/install",
    "prestart:norun": "yarn prestart:config && yarn prestart:database && yarn prestart:init && yarn prestart:package && yarn prestart:install",
    "dev": "NODE_OPTIONS=\"--max_old_space_size=8192\" NEXT_PUBLIC_SOURCE_BRANCH=$(git rev-parse --abbrev-ref HEAD) NEXT_PUBLIC_SOURCE_COMMIT=$(git rev-parse --short HEAD) nodemon",
    "build": "yarn build:server && yarn build:client",
    "build:server": "yarn build:server:config && yarn build:server:src && yarn build:server:models && yarn build:server:plugins && yarn build:server:postprocessing && yarn build:server:templates",
    "build:server:config": "swc config --out-dir dist/config",
    "build:server:src": "swc src --out-dir dist/src",
    "build:server:models": "swc models --out-dir dist/models",
    "build:server:plugins": "swc plugins --out-dir dist/plugins --copy-files",
    "build:server:postprocessing": "swc postprocessing --out-dir dist/postprocessing --copy-files",
    "build:server:templates": "swc templates --out-dir dist/templates --copy-files",
    "build:client": "NEXT_PUBLIC_SOURCE_BRANCH=$(git rev-parse --abbrev-ref HEAD) NEXT_PUBLIC_SOURCE_COMMIT=$(git rev-parse --short HEAD) next build",
    "sitemap": "next-sitemap --config config/sitemap/sitemap.config.mjs",
    "start": "NODE_ENV=production NODE_OPTIONS=\"--max_old_space_size=8192\" node --experimental-specifier-resolution=node dist/src/server/index.js",
    "workflow": ".github/workflows/node.local.sh",
    "modelValidator": "ts-json-schema-generator --path models/index.d.ts --type 'IModel' --tsconfig ./tsconfig.json --no-type-check --out src/components/editor/steps/model.json"
  },
  "dependencies": {
    "electron": "27.1.2",
    "electron-serve": "1.2.0",
    "electron-store": "8.1.0"
  },
  "devDependencies": {
    "@airthium/deep-object-diff": "1.0.20",
    "@airthium/extract-json-from-string": "1.0.38",
    "@airthium/pg-format": "2.0.43",
    "@airthium/tanatloc-3d": "1.0.8",
    "@airthium/tanatloc-formula-validator": "1.0.22",
    "@airthium/typedoc-plugin-airthium": "1.1.24",
    "@ant-design/icons": "5.2.6",
    "@babel/core": "7.23.3",
    "@hapi/iron": "7.0.1",
    "@sentry/nextjs": "7.81.1",
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.99",
    "@testing-library/react": "14.1.2",
    "@types/archiver": "6.0.2",
    "@types/cookie": "0.6.0",
    "@types/ejs": "3.1.5",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.10",
    "@types/lodash": "4.14.202",
    "@types/node": "20.10.0",
    "@types/passport": "1.0.16",
    "@types/passport-local": "1.0.38",
    "@types/pg": "8.10.9",
    "@types/react": "18.2.38",
    "@types/set-interval-async": "1.0.3",
    "@types/sharp": "0.32.0",
    "@types/tar": "6.1.10",
    "@types/three": "0.158.3",
    "@types/url-join": "4.0.3",
    "@types/uuid": "9.0.7",
    "ajv": "8.12.0",
    "antd": "5.11.4",
    "archiver": "6.0.1",
    "babel-jest": "29.7.0",
    "cookie": "0.6.0",
    "depcheck": "1.4.7",
    "ejs": "3.1.9",
    "email-addresses": "5.0.0",
    "eslint": "8.54.0",
    "eslint-config-next": "14.0.3",
    "express": "4.18.2",
    "form-data": "4.0.0",
    "html-react-parser": "5.0.6",
    "https-proxy-agent": "7.0.2",
    "identity-obj-proxy": "3.0.0",
    "ignore-loader": "0.1.2",
    "is-docker": "3.0.0",
    "is-electron": "2.2.2",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "json5": "2.2.3",
    "lodash": "4.17.21",
    "mailersend": "2.2.0",
    "mathjax": "3.2.2",
    "next": "14.0.2",
    "next-sitemap": "4.2.3",
    "node-fetch": "3.3.2",
    "nodemon": "3.0.1",
    "passport": "0.6.0",
    "passport-local": "1.0.0",
    "pg": "8.11.3",
    "pg-native": "npm:@airthium/pg-native@3.0.2",
    "prettier": "3.1.0",
    "react": "18.2.0",
    "react-ace": "10.1.0",
    "react-cookie": "6.1.1",
    "react-dom": "18.2.0",
    "recharts": "2.10.1",
    "set-interval-async": "3.0.3",
    "sharp": "0.32.6",
    "swr": "2.2.4",
    "tar": "6.2.0",
    "three": "0.158.0",
    "ts-json-schema-generator": "1.4.0",
    "typedoc": "0.25.4",
    "typescript": "5.3.2",
    "url-join": "5.0.0",
    "uuid": "9.0.1"
  },
  "packageManager": "yarn@4.0.2"
}
