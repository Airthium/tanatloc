{
  "name": "tanatloc",
  "description": "Tanatloc",
  "version": "1.0.0-alpha.2",
  "repository": "git@github.com:Airthium/tanatloc-ssr.git",
  "license": "UNLICENSED",
  "private": true,
  "homepage": "./",
  "author": {
    "name": "Airthium",
    "url": "https://github.com/orgs/Airthium/people"
  },
  "main": "app/background.js",
  "browser": {
    "fs": false,
    "path": false
  },
  "scripts": {
    "build": "NEXT_PUBLIC_SOURCE_BRANCH=`git rev-parse --abbrev-ref HEAD` NEXT_PUBLIC_SOURCE_COMMIT=`git rev-parse --short HEAD` next build",
    "depcheck": "depcheck",
    "dev": "NODE_OPTIONS=\"--max_old_space_size=8192\" NEXT_PUBLIC_SOURCE_BRANCH=`git rev-parse --abbrev-ref HEAD` NEXT_PUBLIC_SOURCE_COMMIT=`git rev-parse --short HEAD` next",
    "doc": "jsdoc -c config/jsdoc/conf.js",
    "docker": "docker build . -t tanatloc/tanatloc:latest",
    "electron-build": "yarn postinstall && yarn server-build && nextron build && electron-builder install-app-deps",
    "electron-dev": "yarn postinstall && yarn server-build && nextron",
    "export": "yarn build && next export",
    "postinstall": "babel . --ignore \"**/__tests__/**/*.js\" --only config,install,src/database --out-dir dist-install && node dist-install/install",
    "prettier": "prettier -l '**/*.{js,json,md,css}'",
    "prettier:write": "prettier --write '**/*.{js,json,md,css}'",
    "server-build": "babel . --ignore \"**/__tests__/**/*.js\" --only config,models,modules,plugins,server,src/auth,src/database,src/lib,src/route,src/services,templates --out-dir dist-server",
    "start": "NODE_OPTIONS=\"--max_old_space_size=8192\" next start",
    "test": "jest --coverage --silent",
    "testwatch": "jest --watch",
    "teste2e": "DB_DATABASE=tanatloc_test STORAGE_PATH=/tmp/tanatloc_test yarn postinstall && jest --config jest.e2e.config.js --coverage --silent --runInBand",
    "teste2ewatch": "DB_DATABASE=tanatloc_test STORAGE_PATH=/tmp/tanatloc_test yarn postinstall && jest --config jest.e2e.config.js --watch --runInBand",
    "workflow": ".github/workflows/node.local.sh"
  },
  "dependencies": {
    "electron-serve": "1.1.0",
    "electron-store": "8.0.1"
  },
  "devDependencies": {
    "@ant-design/icons": "4.7.0",
    "@babel/cli": "7.15.7",
    "@babel/core": "7.15.5",
    "@hapi/iron": "6.0.0",
    "@reduxjs/toolkit": "1.6.1",
    "@sentry/browser": "6.13.2",
    "@sentry/node": "6.13.2",
    "@testing-library/react": "12.1.1",
    "air-jsdoc-template": "../air-jsdoc-template/",
    "antd": "4.16.13",
    "archiver": "5.3.0",
    "babel-jest": "27.2.3",
    "babel-plugin-import": "1.13.3",
    "babel-plugin-module-resolver": "4.1.0",
    "better-react-mathjax": "1.0.3",
    "cookie": "0.4.1",
    "cors": "2.8.5",
    "deep-object-diff": "1.1.0",
    "depcheck": "1.4.2",
    "ejs": "3.1.6",
    "electron": "15.0.0",
    "electron-builder": "22.11.7",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "express": "4.17.1",
    "http-errors": "1.8.0",
    "ignore-loader": "0.1.2",
    "is-docker": "2.2.1",
    "is-electron": "2.2.0",
    "jest": "27.2.3",
    "jsdoc": "3.6.7",
    "less": "4.1.1",
    "less-loader": "10.0.1",
    "lodash.merge": "4.6.2",
    "mailersend": "1.1.1",
    "ncp": "2.0.0",
    "next": "11.1.2",
    "next-transpile-modules": "8.0.0",
    "next-with-less": "1.0.1",
    "nextron": "7.1.0",
    "node-fetch": "2.6.1",
    "passport": "0.5.0",
    "passport-local": "1.0.0",
    "pg": "8.7.1",
    "pg-native": "./modules/pg-native",
    "prettier": "2.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.5",
    "recharts": "2.1.4",
    "redux": "4.1.1",
    "redux-devtools-extension": "2.13.9",
    "redux-persist": "6.0.0",
    "set-interval-async": "2.0.3",
    "swr": "1.0.1",
    "three": "0.132.2",
    "three-to-glb": "./modules/three-to-glb/",
    "url-join": "4.0.1",
    "uuid": "8.3.2"
  }
}
