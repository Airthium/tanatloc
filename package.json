{
  "name": "tanatloc",
  "version": "1.0.0-beta.1",
  "private": true,
  "description": "Tanatloc",
  "homepage": "./",
  "repository": "git@github.com:Airthium/tanatloc.git",
  "license": "UNLICENSED",
  "author": {
    "name": "Airthium",
    "url": "https://github.com/orgs/Airthium/people"
  },
  "scripts": {
    "depcheck": "node config/depcheck/depcheckrc.js",
    "prettier": "prettier -l '**/*.{js,jsx,ts,tsx,json,md,css}'",
    "prettier:write": "prettier --write '**/*.{js,jsx,ts,tsx,json,md,css}'",
    "doc": "typedoc --options config/typedoc/config.json",
    "testwatch": "jest --config config/jest/jest.config.js --watch",
    "test": "jest --config config/jest/jest.config.js --coverage --silent",
    "sonar": ".github/workflows/sonar.local.sh",
    "prestart": "yarn prestart:config && yarn prestart:database && yarn prestart:init && yarn prestart:install  && yarn prestart:run",
    "prestart:config": "swc config/db.ts --out-dir dist-install/config && swc config/storage.ts --out-dir dist-install/config",
    "prestart:database": "swc src/database/index.ts --out-dir dist-install/src",
    "prestart:init": "swc src/server/init/database.ts --out-dir dist-install/src",
    "prestart:install": "swc install --out-dir dist-install/install",
    "prestart:run": "node dist-install/install",
    "prestart:norun": "yarn prestartconfig && yarn prestartinstall && yarn prestartdatabase && yarn prestartinit",
    "dev": "NODE_OPTIONS=\"--max_old_space_size=8192\" NEXT_PUBLIC_SOURCE_BRANCH=$(git rev-parse --abbrev-ref HEAD) NEXT_PUBLIC_SOURCE_COMMIT=$(git rev-parse --short HEAD) nodemon",
    "build": "yarn build:server && yarn build:client",
    "build:server": "rm -rf dist && yarn build:server:config && yarn build:server:models && yarn build:server:plugins && yarn build:server:src && yarn build:server:templates",
    "build:server:config": "swc config --out-dir dist/config",
    "build:server:models": "swc models --out-dir dist/models",
    "build:server:plugins": "yarn swc plugins --out-dir dist/plugins --copy-files",
    "build:server:src": "swc src --out-dir dist/src --copy-files",
    "build:server:templates": "yarn swc templates --out-dir dist/templates --copy-files",
    "build:client": "NEXT_PUBLIC_SOURCE_BRANCH=$(git rev-parse --abbrev-ref HEAD) NEXT_PUBLIC_SOURCE_COMMIT=$(git rev-parse --short HEAD) next build",
    "export": "yarn build:client && next export",
    "start": "NODE_ENV=production NODE_OPTIONS=\"--max_old_space_size=8192\" node dist/src/server/index.js",
    "preteste2e": "DB_DATABASE=tanatloc_test STORAGE_PATH=/tmp/tanatloc_test yarn prestart",
    "teste2e": "jest --config config/jest/e2e/jest.config.js --coverage --silent --runInBand",
    "teste2ewatch": "jest --config config/jest/e2e/jest.config.js --watch --runInBand",
    "workflow": ".github/workflows/node.local.sh"
  },
  "dependencies": {
    "electron": "18.2.0",
    "electron-serve": "1.1.0",
    "electron-store": "8.0.1"
  },
