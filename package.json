{
  "name": "tanatloc",
  "version": "1.0.0-alpha.16.4",
  "private": true,
  "description": "Tanatloc",
  "homepage": "./",
  "repository": "git@github.com:Airthium/tanatloc.git",
  "license": "UNLICENSED",
  "author": {
    "name": "Airthium",
