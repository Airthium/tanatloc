{
  "name": "tanatloc",
  "description": "Tanatloc",
  "version": "1.0.0-alpha.3",
  "repository": "git@github.com:Airthium/tanatloc-ssr.git",
  "license": "UNLICENSED",
  "private": true,
  "homepage": "./",
  "author": {
    "name": "Airthium",
    "url": "https://github.com/orgs/Airthium/people"
  },
  "main": "app/background.js",
  "browser": {
    "fs": false,
    "path": false
  },
  "scripts": {
    "build": "NEXT_PUBLIC_SOURCE_BRANCH=`git rev-parse --abbrev-ref HEAD` NEXT_PUBLIC_SOURCE_COMMIT=`git rev-parse --short HEAD` next build",
    "depcheck": "depcheck --config=config/depcheck/.depcheckrc",
    "dev": "NODE_OPTIONS=\"--max_old_space_size=8192\" NEXT_PUBLIC_SOURCE_BRANCH=`git rev-parse --abbrev-ref HEAD` NEXT_PUBLIC_SOURCE_COMMIT=`git rev-parse --short HEAD` next",
    "doc": "jsdoc -c config/jsdoc/conf.js",
    "docker": "docker build . -t tanatloc/tanatloc:latest",
    "electron-build": "yarn postinstall && yarn server-build && nextron build && electron-builder install-app-deps",
    "electron-dev": "yarn postinstall && yarn server-build && nextron",
    "export": "yarn build && next export",
    "postinstall": "babel . --extensions '.js,.jsx,.ts,.tsx' --only config,install,src/database --out-dir dist-install && node dist-install/install",
    "prettier": "prettier -l '**/*.{js,jsx,ts,tsx,json,md,css}'",
    "prettier:write": "prettier --write '**/*.{js,jsx,ts,tsx,json,md,css}'",
    "server-build": "babel . --extensions '.js,.jsx,.ts,.tsx' --only config,models,modules,plugins,server,src/auth,src/database,src/lib,src/route,src/services,templates --out-dir dist-server",
    "start": "NODE_OPTIONS=\"--max_old_space_size=8192\" next start",
    "test": "jest --config config/jest/jest.config.js --coverage --silent",
    "testwatch": "jest --config config/jest/jest.config.js --watch",
    "preteste2e": "DB_DATABASE=tanatloc_test STORAGE_PATH=/tmp/tanatloc_test yarn postinstall",
    "teste2e": "jest --config config/jest/e2e/jest.config.js --coverage --silent --runInBand",
    "teste2ewatch": "jest --config config/jest/e2e/jest.config.js --watch --runInBand",
    "workflow": ".github/workflows/node.local.sh"
  },
  "dependencies": {
    "electron-serve": "1.1.0",
    "electron-store": "8.0.1"
  },
  "devDependencies": {
    "@airthium/air-jsdoc-template": "1.0.4",
    "@ant-design/icons": "4.7.0",
    "@babel/cli": "7.16.0",
    "@babel/preset-typescript": "7.16.0",
    "@hapi/iron": "6.0.0",
    "@reduxjs/toolkit": "1.6.2",
    "@sentry/browser": "6.15.0",
    "@sentry/node": "6.15.0",
    "@swc/cli": "0.1.51",
    "@swc/core": "1.2.110",
    "@swc/jest": "0.2.5",
    "@testing-library/react": "12.1.2",
    "@types/jest": "^27.0.2",
    "antd": "4.16.13",
    "archiver": "5.3.0",
    "babel-jest": "27.3.1",
    "babel-plugin-import": "1.13.3",
    "babel-plugin-module-resolver": "4.1.0",
    "better-docs": "2.3.2",
    "better-react-mathjax": "1.0.3",
    "cookie": "0.4.1",
    "cors": "2.8.5",
    "deep-object-diff": "1.1.0",
    "depcheck": "1.4.2",
    "ejs": "3.1.6",
    "electron": "16.0.0",
    "electron-builder": "22.13.1",
    "eslint": "8.2.0",
    "eslint-config-next": "12.0.4",
    "express": "4.17.1",
    "http-errors": "1.8.1",
    "ignore-loader": "0.1.2",
    "is-docker": "2.2.1",
    "is-electron": "2.2.0",
    "jest": "27.3.1",
    "jest-canvas-mock": "2.3.1",
    "jsdoc": "3.6.7",
    "lodash.merge": "4.6.2",
    "mailersend": "1.2.0",
    "next": "12.0.4",
    "next-compose-plugins": "2.2.1",
    "next-plugin-antd-less": "1.5.2",
    "nextron": "7.1.0",
    "node-fetch": "2.6.1",
    "passport": "0.5.0",
    "passport-local": "1.0.0",
    "pg": "8.7.1",
    "pg-native": "./modules/pg-native",
    "prettier": "2.4.1",
    "prism-themes": "1.9.0",
    "prismjs": "1.25.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "react-simple-code-editor": "0.11.0",
    "recharts": "2.1.6",
    "redux": "4.1.2",
    "redux-devtools-extension": "2.13.9",
    "redux-persist": "6.0.0",
    "set-interval-async": "2.0.3",
    "swr": "1.0.1",
    "tar": "6.1.11",
    "three": "0.134.0",
    "three-to-glb": "./modules/three-to-glb/",
    "typedoc": "0.22.9",
    "typescript": "4.5.2",
    "url-join": "4.0.1",
    "uuid": "8.3.2"
  }
}
