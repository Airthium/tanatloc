{
  "name": "tanatloc",
  "version": "1.0.0",
  "description": "Tanatloc",
  "homepage": "./",
  "repository": "git@github.com:Airthium/tanatloc.git",
  "license": "AGPL-3.0-or-later",
  "author": {
    "name": "Airthium",
    "url": "https://github.com/orgs/Airthium/people"
  },
  "scripts": {
    "depcheck": "node config/depcheck/depcheckrc.js",
    "prettier": "prettier -l '**/*.{js,jsx,ts,tsx,json,md,css}'",
    "prettier:write": "prettier --write '**/*.{js,jsx,ts,tsx,json,md,css}'",
    "doc": "typedoc --options config/typedoc/config.json",
    "testwatch": "jest --config config/jest/jest.config.js --watch",
    "test": "jest --config config/jest/jest.config.js --coverage --silent",
    "sonar": ".github/workflows/sonar.local.sh",
    "prestart": "yarn prestart:config && yarn prestart:database && yarn prestart:init && yarn prestart:install && yarn prestart:run",
    "prestart:config": "swc config/db.ts --out-dir dist-install/config && swc config/storage.ts --out-dir dist-install/config",
    "prestart:database": "swc src/database/index.ts --out-dir dist-install/src",
    "prestart:init": "swc src/server/init/database.ts --out-dir dist-install/src",
    "prestart:install": "swc install --out-dir dist-install/install",
    "prestart:run": "node dist-install/install",
    "prestart:norun": "yarn prestart:config && yarn prestart:database && yarn prestart:init && yarn prestart:install",
    "dev": "NODE_OPTIONS=\"--max_old_space_size=8192\" NEXT_PUBLIC_SOURCE_BRANCH=$(git rev-parse --abbrev-ref HEAD) NEXT_PUBLIC_SOURCE_COMMIT=$(git rev-parse --short HEAD) nodemon",
    "build": "yarn build:server && yarn build:client",
    "build:server": "yarn build:server:config && yarn build:server:src && yarn build:server:models && yarn build:server:plugins && yarn build:server:postprocessing && yarn build:server:templates",
    "build:server:config": "swc config --out-dir dist/config",
    "build:server:src": "swc src --out-dir dist/src",
    "build:server:models": "swc models --out-dir dist/models",
    "build:server:plugins": "swc plugins --out-dir dist/plugins --copy-files",
    "build:server:postprocessing": "swc postprocessing --out-dir dist/postprocessing --copy-files",
    "build:server:templates": "swc templates --out-dir dist/templates --copy-files",
    "build:client": "NEXT_PUBLIC_SOURCE_BRANCH=$(git rev-parse --abbrev-ref HEAD) NEXT_PUBLIC_SOURCE_COMMIT=$(git rev-parse --short HEAD) next build",
    "export": "yarn build:client && next export",
    "start": "NODE_ENV=production NODE_OPTIONS=\"--max_old_space_size=8192\" node dist/src/server/index.js",
    "workflow": ".github/workflows/node.local.sh"
  },
  "dependencies": {
    "electron": "20.1.4",
    "electron-serve": "1.1.0",
    "electron-store": "8.1.0"
  },
  "devDependencies": {
    "@ant-design/icons": "4.7.0",
    "@babel/core": "7.19.1",
    "@hapi/iron": "7.0.0",
    "@sentry/browser": "7.13.0",
    "@sentry/node": "7.13.0",
    "@swc/cli": "0.1.57",
    "@swc/core": "1.3.1",
    "@testing-library/react": "13.4.0",
    "@types/archiver": "5.3.1",
    "@types/cookie": "0.5.1",
    "@types/ejs": "3.1.1",
    "@types/express": "4.17.14",
    "@types/jest": "29.0.3",
    "@types/lodash": "4.14.185",
    "@types/node": "17.0.2",
    "@types/passport": "1.0.11",
    "@types/passport-local": "1.0.34",
    "@types/pg": "8.6.5",
    "@types/react": "18.0.20",
    "@types/set-interval-async": "1.0.0",
    "@types/sharp": "0.31.0",
    "@types/tar": "6.1.2",
    "@types/three": "0.144.0",
    "@types/url-join": "4.0.1",
    "@types/uuid": "8.3.4",
    "antd": "4.23.2",
    "archiver": "5.3.1",
    "babel-jest": "29.0.3",
    "cookie": "0.5.0",
    "deep-object-diff": "1.1.7",
    "depcheck": "1.4.3",
    "ejs": "3.1.8",
    "email-addresses": "5.0.0",
    "eslint": "8.23.1",
    "eslint-config-next": "12.3.0",
    "express": "4.18.1",
    "extract-json-from-string": "1.0.1",
    "form-data": "4.0.0",
    "html-react-parser": "3.0.4",
    "https-proxy-agent": "5.0.1",
    "ignore-loader": "0.1.2",
    "is-docker": "2.2.1",
    "is-electron": "2.2.1",
    "jest": "29.0.3",
    "jest-environment-jsdom": "29.0.3",
    "lodash": "4.17.21",
    "mailersend": "1.4.1",
    "mathjax": "3.2.2",
    "next": "12.3.0",
    "next-plugin-antd-less": "1.8.0",
    "next-transpile-modules": "9.0.0",
    "node-fetch": "3.2.10",
    "nodemon": "2.0.20",
    "passport": "0.6.0",
    "passport-local": "1.0.0",
    "pg": "8.8.0",
    "pg-format": "https://github.com/Airthium/pg-format.git#1.0.8",
    "pg-native": "https://github.com/Airthium/pg-native.git#3.0.1",
    "prettier": "2.7.1",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recharts": "2.1.12",
    "set-interval-async": "3.0.2",
    "sharp": "0.31.0",
    "swr": "1.3.0",
    "tar": "6.1.11",
    "three": "0.144.0",
    "typedoc": "0.23.15",
    "typedoc-plugin-airthium": "https://github.com/Airthium/typedoc-plugin-airthium.git#1.0.6",
    "typescript": "4.8.3",
    "url-join": "4.0.1",
    "uuid": "9.0.0"
  },
  "packageManager": "yarn@3.2.3"
}
