{
  "name": "tanatloc",
  "version": "1.0.0",
  "private": true,
  "homepage": "./",
  "main": "main/index.js",
  "scripts": {
    "prettier": "prettier -l '**/*.{js,json,md,css}'",
    "prettier:write": "prettier --write '**/*.{js,json,md,css}'",
    "depcheck": "depcheck",
    "dev": "next renderer",
    "build": "next build renderer",
    "start": "next start renderer",
    "export": "yarn build && next export renderer",
    "server-build": "babel . --only config,renderer/pages/api,server,src --out-dir dist-server",
    "electron-dev": "electron .",
    "electron-build": "yarn export && yarn server-build && electron-builder"
  },
  "build": {
    "asar": true,
    "files": [
      "main",
      "src",
      "renderer/out"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "4.2.1",
    "@babel/cli": "7.10.5",
    "@babel/core": "7.10.5",
    "@babel/preset-env": "7.10.4",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-less": "1.0.1",
    "antd": "4.5.1",
    "babel-plugin-import": "1.13.0",
    "cookie-parser": "1.4.5",
    "drei": "0.0.66",
    "electron-next": "3.1.5",
    "express": "4.17.1",
    "less": "3.12.2",
    "next": "latest",
    "next-transpile-modules": "4.0.0",
    "pg": "8.3.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.1",
    "react-three-fiber": "4.2.17",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-persist": "6.0.0",
    "three": "0.118.3"
  },
  "browser": {
    "fs": false,
    "path": false
  },
  "devDependencies": {
    "electron": "9.1.1",
    "electron-builder": "22.8.0",
    "prettier": "2.0.5"
  }
}
