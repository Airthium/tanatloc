{
  "name": "tanatloc",
  "description": "Tanatloc",
  "version": "1.0.0-alpha.7",
  "repository": "git@github.com:Airthium/tanatloc.git",
  "license": "UNLICENSED",
  "private": true,
  "homepage": "./",
  "author": {
    "name": "Airthium",
    "url": "https://github.com/orgs/Airthium/people"
  },
  "browser": {
    "fs": false,
    "path": false
  },
  "scripts": {
    "build": "NEXT_PUBLIC_SOURCE_BRANCH=`git rev-parse --abbrev-ref HEAD` NEXT_PUBLIC_SOURCE_COMMIT=`git rev-parse --short HEAD` next build",
    "depcheck": "node config/depcheck/depcheckrc.js",
    "dev": "NODE_OPTIONS=\"--max_old_space_size=8192\" NEXT_PUBLIC_SOURCE_BRANCH=`git rev-parse --abbrev-ref HEAD` NEXT_PUBLIC_SOURCE_COMMIT=`git rev-parse --short HEAD` next",
    "doc": "typedoc --options config/typedoc/config.json",
    "export": "yarn build && next export",
    "prestart": "yarn prestartconfig && yarn prestartinstall && yarn prestartdatabase && yarn prestartrun",
    "prestartwithoutrun": "yarn prestartconfig && yarn prestartinstall && yarn prestartdatabase",
    "prestartconfig": "swc config --out-dir dist-install/config",
    "prestartinstall": "swc install --out-dir dist-install/install",
    "prestartdatabase": "swc src/database --out-dir dist-install/src",
    "prestartrun": "node dist-install/install",
    "prettier": "prettier -l '**/*.{js,jsx,ts,tsx,json,md,css}'",
    "prettier:write": "prettier --write '**/*.{js,jsx,ts,tsx,json,md,css}'",
    "start": "NODE_OPTIONS=\"--max_old_space_size=8192\" next start",
    "test": "jest --config config/jest/jest.config.js --coverage --silent",
    "testwatch": "jest --config config/jest/jest.config.js --watch",
    "preteste2e": "DB_DATABASE=tanatloc_test STORAGE_PATH=/tmp/tanatloc_test yarn prestart",
    "teste2e": "jest --config config/jest/e2e/jest.config.js --coverage --silent --runInBand",
    "teste2ewatch": "jest --config config/jest/e2e/jest.config.js --watch --runInBand",
    "workflow": ".github/workflows/node.local.sh"
  },
  "dependencies": {
    "electron": "16.0.4",
    "electron-serve": "1.1.0",
    "electron-store": "8.0.1"
  },
  "devDependencies": {
    "@ant-design/icons": "4.7.0",
    "@hapi/iron": "6.0.0",
    "@reduxjs/toolkit": "1.7.0",
    "@sentry/browser": "6.16.1",
    "@sentry/node": "6.16.1",
    "@swc/cli": "0.1.55",
    "@swc/core": "1.2.120",
    "@swc/jest": "0.2.15",
    "@testing-library/react": "12.1.2",
    "@types/jest": "27.0.3",
    "antd": "4.17.3",
    "archiver": "5.3.0",
    "better-react-mathjax": "1.0.3",
    "cookie": "0.4.1",
    "deep-object-diff": "1.1.0",
    "depcheck": "1.4.2",
    "ejs": "3.1.6",
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7",
    "express": "4.17.1",
    "form-data": "4.0.0",
    "ignore-loader": "0.1.2",
    "is-docker": "3.0.0",
    "is-electron": "2.2.1",
    "jest": "27.4.5",
    "jest-canvas-mock": "2.3.1",
    "lodash.merge": "4.6.2",
    "mailersend": "1.3.1",
    "next": "12.0.7",
    "next-compose-plugins": "2.2.1",
    "next-plugin-antd-less": "1.6.0",
    "node-fetch": "3.1.0",
    "passport": "0.5.0",
    "passport-local": "1.0.0",
    "pg": "8.7.1",
    "pg-native": "./modules/pg-native",
    "prettier": "2.5.1",
    "prism-themes": "1.9.0",
    "prismjs": "1.25.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "react-simple-code-editor": "0.11.0",
    "recharts": "2.1.8",
    "redux": "4.1.2",
    "redux-devtools-extension": "2.13.9",
    "redux-persist": "6.0.0",
    "set-interval-async": "2.0.3",
    "swr": "1.1.1",
    "tar": "6.1.11",
    "three": "0.135.0",
    "three-to-glb": "./modules/three-to-glb/",
    "typedoc": "0.22.10",
    "typescript": "4.5.4",
    "url-join": "4.0.1",
    "uuid": "8.3.2"
  }
}
