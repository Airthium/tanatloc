{
  "name": "tanatloc",
  "version": "0.9.0",
  "private": true,
  "description": "Tanatloc",
  "homepage": "./",
  "author": {
    "name": "Airthium",
    "url": "https://github.com/orgs/Airthium/people"
  },
  "main": "app/background.js",
  "browser": {
    "fs": false,
    "path": false
  },
  "scripts": {
    "build": "next build renderer",
    "clean": "rimraf dist/ dist-server/ renderer/out/ renderer/.next/",
    "depcheck": "depcheck",
    "dev": "next renderer",
    "doc": "jsdoc -c config/jsdoc/conf.json",
    "electron-build": "yarn server-build && nextron build && electron-builder install-app-deps",
    "electron-dev": "yarn server-build && nextron",
    "export": "yarn build && next export renderer",
    "postinstall": "babel . --only config,install,src/database/index.js --out-dir dist-install && node dist-install/install",
    "prettier": "prettier -l '**/*.{js,json,md,css}'",
    "prettier:write": "prettier --write '**/*.{js,json,md,css}'",
    "server-build": "babel . --only config,renderer/pages/api,server,src --out-dir dist-server",
    "start": "next start renderer",
    "test": "jest --coverage --silent",
    "workflow": ".github/workflows/node.local.sh"
  },
  "dependencies": {
    "electron-serve": "1.0.0",
    "electron-store": "6.0.0"
  },
  "devDependencies": {
    "@ant-design/icons": "4.2.2",
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/preset-typescript": "7.10.4",
    "@hapi/iron": "6.0.0",
    "@sentry/browser": "5.23.0",
    "@sentry/node": "5.23.0",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-less": "1.0.1",
    "antd": "4.6.3",
    "babel-jest": "26.3.0",
    "babel-loader": "8.1.0",
    "babel-plugin-import": "1.13.0",
    "cookie": "0.4.1",
    "cors": "2.8.5",
    "depcheck": "1.2.0",
    "electron": "10.1.1",
    "electron-builder": "22.8.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.4",
    "express": "4.17.1",
    "foodoc": "^0.0.9",
    "http-errors": "1.8.0",
    "is-electron": "2.2.0",
    "jest": "26.4.2",
    "jsdoc": "3.6.5",
    "less": "3.12.2",
    "next": "9.3.6",
    "next-transpile-modules": "4.1.0",
    "nextron": "5.15.1",
    "passport": "0.4.1",
    "passport-local": "1.0.0",
    "pg": "8.3.3",
    "pg-native": "3.0.0",
    "prettier": "2.1.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.1",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-persist": "6.0.0",
    "rimraf": "3.0.2",
    "swr": "0.3.2",
    "three": "0.120.1"
  }
}
