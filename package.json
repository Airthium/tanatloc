{
  "name": "tanatloc",
  "version": "0.9.0",
  "private": true,
  "description": "Tanatloc",
  "homepage": "./",
  "author": {
    "name": "Airthium",
    "url": "https://github.com/orgs/Airthium/people"
  },
  "main": "app/background.js",
  "browser": {
    "fs": false,
    "path": false
  },
  "scripts": {
    "build": "next build renderer",
    "clean": "rimraf dist/ dist-server/ renderer/out/ renderer/.next/",
    "depcheck": "depcheck",
    "dev": "next renderer",
    "doc": "jsdoc -c config/jsdoc/conf.json",
    "electron-build": "patch-package && yarn server-build && nextron build && electron-builder install-app-deps",
    "electron-dev": "yarn server-build && nextron",
    "export": "yarn build && next export renderer",
    "postinstall": "babel . --only config,install,src/database/index.js --out-dir dist-install && node dist-install/install",
    "prettier": "prettier -l '**/*.{js,json,md,css}'",
    "prettier:write": "prettier --write '**/*.{js,json,md,css}'",
    "server-build": "babel . --only config,renderer/pages/api,server,src --out-dir dist-server",
    "start": "next start renderer",
    "test": "jest --coverage --silent",
    "workflow": ".github/workflows/node.local.sh"
  },
  "dependencies": {
    "electron-serve": "1.0.0",
    "electron-store": "6.0.1"
  },
  "devDependencies": {
    "@ant-design/icons": "4.3.0",
    "@babel/cli": "7.12.8",
    "@babel/core": "7.12.9",
    "@babel/preset-typescript": "7.12.7",
    "@hapi/iron": "6.0.0",
    "@sentry/browser": "5.28.0",
    "@sentry/node": "5.28.0",
    "@wojtekmaj/enzyme-adapter-react-17": "0.3.2",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-less": "1.0.1",
    "antd": "4.9.1",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-import": "1.13.3",
    "cookie": "0.4.1",
    "cors": "2.8.5",
    "depcheck": "1.3.1",
    "electron": "11.0.3",
    "electron-builder": "22.9.1",
    "enzyme": "3.11.0",
    "express": "4.17.1",
    "foodoc": "0.0.9",
    "http-errors": "1.8.0",
    "is-electron": "2.2.0",
    "jest": "26.6.3",
    "jsdoc": "3.6.6",
    "less": "3.12.2",
    "next": "10.0.3",
    "next-transpile-modules": "6.0.0",
    "nextron": "6.0.1",
    "passport": "0.4.1",
    "passport-local": "1.0.0",
    "patch-package": "6.2.2",
    "pg": "8.5.1",
    "pg-native": "3.0.0",
    "prettier": "2.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "7.2.2",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-persist": "6.0.0",
    "rimraf": "3.0.2",
    "swr": "0.3.9",
    "three": "0.123.0"
  }
}
