{
  "name": "tanatloc",
  "version": "0.9.0",
  "private": true,
  "license": "UNLICENSED",
  "description": "Tanatloc",
  "homepage": "./",
  "author": {
    "name": "Airthium",
    "url": "https://github.com/orgs/Airthium/people"
  },
  "main": "app/background.js",
  "browser": {
    "fs": false,
    "path": false
  },
  "scripts": {
    "build": "next build",
    "clean": "rimraf dist/ dist-server/ out/ .next/",
    "copyassets": "mkdir -p public/three/libs/draco && cp -Rf node_modules/three/examples/js/libs/draco/* public/three/libs/draco/",
    "depcheck": "depcheck",
    "dev": "next",
    "doc": "jsdoc -c config/jsdoc/conf.json",
    "docker": "docker build . -t tanatloc/tanatloc:latest",
    "electron-build": "yarn server-build && nextron build && electron-builder install-app-deps",
    "electron-dev": "yarn server-build && nextron",
    "export": "yarn build && next export",
    "postinstall": "babel . --only config,install,src/database/index.js --out-dir dist-install && node dist-install/install && yarn copyassets",
    "prettier": "prettier -l '**/*.{js,json,md,css}'",
    "prettier:write": "prettier --write '**/*.{js,json,md,css}'",
    "server-build": "babel . --only config,server,plugin,src/auth,src/database,src/lib,src/route,src/services --out-dir dist-server",
    "start": "next start",
    "test": "jest --coverage --silent",
    "workflow": ".github/workflows/node.local.sh"
  },
  "dependencies": {
    "electron-serve": "1.1.0",
    "electron-store": "8.0.0"
  },
  "devDependencies": {
    "@ant-design/icons": "4.6.2",
    "@babel/cli": "7.13.16",
    "@babel/core": "7.14.0",
    "@babel/preset-typescript": "7.13.0",
    "@hapi/iron": "6.0.0",
    "@sentry/browser": "6.3.4",
    "@sentry/node": "6.3.4",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.1",
    "@zeit/next-less": "1.0.1",
    "antd": "4.15.3",
    "archiver": "5.3.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-import": "1.13.3",
    "babel-plugin-module-resolver": "4.1.0",
    "cookie": "0.4.1",
    "cors": "2.8.5",
    "deep-object-diff": "1.1.0",
    "depcheck": "1.4.0",
    "ejs": "3.1.6",
    "electron": "12.0.5",
    "electron-builder": "22.10.5",
    "enzyme": "3.11.0",
    "express": "4.17.1",
    "foodoc": "0.0.9",
    "http-errors": "1.8.0",
    "is-docker": "2.2.1",
    "is-electron": "2.2.0",
    "jest": "26.6.3",
    "jsdoc": "3.6.6",
    "less": "4.1.1",
    "lodash.merge": "4.6.2",
    "mailersend": "1.0.2",
    "next": "10.2.0",
    "next-transpile-modules": "7.0.0",
    "nextron": "6.0.6",
    "node-fetch": "2.6.1",
    "passport": "0.4.1",
    "passport-local": "1.0.0",
    "pg": "8.6.0",
    "pg-native": "3.0.0",
    "prettier": "2.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.4",
    "recharts": "2.0.9",
    "redux": "4.1.0",
    "redux-devtools-extension": "2.13.9",
    "redux-persist": "6.0.0",
    "rimraf": "3.0.2",
    "swr": "0.5.5",
    "three": "0.128.0",
    "three-to-glb": "./modules/three-to-glb/",
    "url-join": "4.0.1"
  }
}
