{"ast":null,"code":"import _Card from \"antd/lib/card\";\nimport { jsx as _jsx } from \"react/jsx-runtime\";\n\n/** @module renderer/components/assets/selector */\nimport { useEffect } from 'react';\nimport { useSelector, useDispatch } from 'react-redux';\nimport { highlight, unhighlight, select, unselect } from '../../../store/select/action';\n/**\n * Selector\n * @param {Object} props Props\n */\n\nvar Selector = function Selector(_ref) {\n  var part = _ref.part,\n      updateSelected = _ref.updateSelected;\n\n  // Store\n  var _useSelector = useSelector(function (state) {\n    return {\n      type: state.select.type,\n      highlighted: state.select.highlighted,\n      selected: state.select.selected\n    };\n  }),\n      type = _useSelector.type,\n      highlighted = _useSelector.highlighted,\n      selected = _useSelector.selected;\n\n  var dispatch = useDispatch(); // Selected\n\n  useEffect(function () {\n    updateSelected(selected);\n  }, [selected]);\n  /**\n   * On highglight\n   * @param {string} uuid UUID\n   */\n\n  var onHighlight = function onHighlight(uuid) {\n    dispatch(highlight(uuid));\n  };\n  /**\n   * On unhighlight\n   */\n\n\n  var onUnhighlight = function onUnhighlight() {\n    dispatch(unhighlight());\n  };\n  /**\n   * On select\n   * @param {string} uuid UUID\n   */\n\n\n  var onSelect = function onSelect(uuid) {\n    if (selected.includes(uuid)) dispatch(unselect(uuid));else dispatch(select(uuid));\n  };\n  /**\n   * Render\n   */\n\n\n  return part !== null && part !== void 0 && part[type] ? part[type].map(function (element, index) {\n    return /*#__PURE__*/_jsx(_Card, {\n      hoverable: true,\n      style: {\n        marginBottom: highlighted === element.uuid ? '5px' : selected.includes(element.uuid) ? '5px' : '7px',\n        border: highlighted === element.uuid ? '2px solid #0096C7' : selected.includes(element.uuid) ? '2px solid #c73100' : '1px solid grey'\n      },\n      bodyStyle: {\n        padding: '10px'\n      },\n      onMouseEnter: function onMouseEnter() {\n        return onHighlight(element.uuid);\n      },\n      onMouseLeave: onUnhighlight,\n      onClick: function onClick() {\n        return onSelect(element.uuid);\n      },\n      children: element.name\n    }, index);\n  }) : [];\n};\n\nexport default Selector;","map":null,"metadata":{},"sourceType":"module"}