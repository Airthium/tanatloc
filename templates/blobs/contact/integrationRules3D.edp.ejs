// Integration points number by segment
int ipn;
// Positions
real[int] tip1, tip2;
// Weights
real[int] wip;

func int iprule(string integrationRule) {
  if (integrationRule == "Gauss") {
    // Gauss quadrature IP
    ipn = 7;
    tip1 = [
        1. / 3.,
        (6. - 15^0.5) / 21.,
        (6. - 15^0.5) / 21.,
        (9. + 2 * 15^0.5) / 21.,
        (6. + 15^0.5) / 21.,
        (6. + 15^0.5) / 21.,
        (9. - 2 * 15^0.5) / 21.
        ];
    tip2 = [
        1. / 3.,
        (6. - 15^0.5) / 21.,
        (9. + 2 * 15^0.5) / 21.,
        (6. - 15^0.5) / 21.,
        (6. + 15^0.5) / 21.,
        (9. - 2 * 15^0.5) / 21.,
        (6. + 15^0.5) / 21.
        ];
    wip = 1 / 2. * [
        0.225, (155. - 15^0.5) / 1200.,
        (155. - 15^0.5) / 1200.,
        (155. - 15^0.5) / 1200.,
        (155. + 15^0.5) / 1200.,
        (155. + 15^0.5) / 1200.,
        (155. + 15^0.5) / 1200.
        ];
  }

  return 0;
}
