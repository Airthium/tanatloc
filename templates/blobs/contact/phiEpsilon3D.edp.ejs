// Phi_{epsilon} function approximating the vector module

// Definition of the function Phi_{epsilon}
func real phieps(real xx, real yy, real zz, real epsil) {
    real resu;
    resu = (xx^2 + yy^2 + zz^2 + epsil)^0.5;
    return resu;
}

// ax Derivative of the function Phi_{epsilon}
func real axphieps(real xx, real yy, real zz, real epsil) {
    real resu;
    resu = xx / ((xx^2 + yy^2 + zz^2 + epsil)^0.5);
    return resu;
}

// ay Derivative of the function Phi_{epsilon}
func real ayphieps(real xx, real yy, real zz, real epsil) {
    real resu;
    resu = yy / ((xx^2 + yy^2 + zz^2 + epsil)^0.5);
    return resu;
}

// az Derivative of the function Phi_{epsilon}
func real azphieps(real xx, real yy, real zz, real epsil) {
    real resu;
    resu = zz / ((xx^2 + yy^2 + zz^2 + epsil)^0.5);
    return resu;
}

// axx Derivative of the function Phi_{epsilon}
func real axxphieps(real xx, real yy, real zz, real epsil) {
    real resu;
    resu = (yy^2 + zz^2 + epsil) / (((xx^2 + yy^2 + zz^2 + epsil)^0.5) * (xx^2 + yy^2 + zz^2 + epsil));
    return resu;
}

// ayy Derivative of the function Phi_{epsilon}
func real ayyphieps(real xx, real yy, real zz, real epsil) {
    real resu;
    resu = (xx^2 + zz^2 + epsil) / (((xx^2 + yy^2 + zz^2 + epsil)^0.5) * (xx^2 + yy^2 + zz^2 + epsil));
    return resu;
}

// azz Derivative of the function Phi_{epsilon}
func real azzphieps(real xx, real yy, real zz, real epsil) {
    real resu;
    resu = (xx^2 + yy^2 + epsil) / (((xx^2 + yy^2 + zz^2 + epsil)^0.5) * (xx^2 + yy^2 + zz^2 + epsil));
    return resu;
}

// axy Derivative of the function Phi_{epsilon}
func real axyphieps(real xx, real yy, real zz, real epsil) {
    real resu;
    resu = - xx * yy / (((xx^2 + yy^2 + zz^2 + epsil)^0.5) * (xx^2 + yy^2 + zz^2 + epsil));
    return resu;
}

// axz Derivative of the function Phi_{epsilon}
func real axzphieps(real xx, real yy, real zz, real epsil) {
    real resu;
    resu = - xx * zz / (((xx^2 + yy^2 + zz^2 + epsil)^0.5) * (xx^2 + yy^2 + zz^2 + epsil));
    return resu;
}

// ayz Derivative of the function Phi_{epsilon}
func real ayzphieps(real xx, real yy, real zz, real epsil) {
    real resu;
    resu = - yy * zz / (((xx^2 + yy^2 + zz^2 + epsil)^0.5) * (xx^2 + yy^2 + zz^2 + epsil));
    return resu;
}

