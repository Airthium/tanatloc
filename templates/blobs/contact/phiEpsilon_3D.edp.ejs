// Phi_{epsilon} function approximating the vector module

// Definition of the function Phi_{epsilon}
func real phiEpsilon(real xx, real yy, real zz, real epsilon) {
    return (xx^2 + yy^2 + zz^2 + epsilon)^0.5;
}

// ax Derivative of the function Phi_{epsilon}
func real axPhiEpsilon(real xx, real yy, real zz, real epsilon) {
    return xx / ((xx^2 + yy^2 + zz^2 + epsilon)^0.5);
}

// ay Derivative of the function Phi_{epsilon}
func real ayPhiEpsilon(real xx, real yy, real zz, real epsilon) {
    return yy / ((xx^2 + yy^2 + zz^2 + epsilon)^0.5);
}

// az Derivative of the function Phi_{epsilon}
func real azPhiEpsilon(real xx, real yy, real zz, real epsilon) {
    return zz / ((xx^2 + yy^2 + zz^2 + epsilon)^0.5);
}

// axx Derivative of the function Phi_{epsilon}
func real axxPhiEpsilon(real xx, real yy, real zz, real epsilon) {
    return (yy^2 + zz^2 + epsilon) / (((xx^2 + yy^2 + zz^2 + epsilon)^0.5) * (xx^2 + yy^2 + zz^2 + epsilon));
}

// ayy Derivative of the function Phi_{epsilon}
func real ayyPhiEpsilon(real xx, real yy, real zz, real epsilon) {
    return (xx^2 + zz^2 + epsilon) / (((xx^2 + yy^2 + zz^2 + epsilon)^0.5) * (xx^2 + yy^2 + zz^2 + epsilon));
}

// azz Derivative of the function Phi_{epsilon}
func real azzPhiEpsilon(real xx, real yy, real zz, real epsilon) {
    return (xx^2 + yy^2 + epsilon) / (((xx^2 + yy^2 + zz^2 + epsilon)^0.5) * (xx^2 + yy^2 + zz^2 + epsilon));
}

// axy Derivative of the function Phi_{epsilon}
func real axyPhiEpsilon(real xx, real yy, real zz, real epsilon) {
    return -xx * yy / (((xx^2 + yy^2 + zz^2 + epsilon)^0.5) * (xx^2 + yy^2 + zz^2 + epsilon));
}

// axz Derivative of the function Phi_{epsilon}
func real axzPhiEpsilon(real xx, real yy, real zz, real epsilon) {
    return -xx * zz / (((xx^2 + yy^2 + zz^2 + epsilon)^0.5) * (xx^2 + yy^2 + zz^2 + epsilon));
}

// ayz Derivative of the function Phi_{epsilon}
func real ayzPhiEpsilon(real xx, real yy, real zz, real epsilon) {
    return -yy * zz / (((xx^2 + yy^2 + zz^2 + epsilon)^0.5) * (xx^2 + yy^2 + zz^2 + epsilon));
}