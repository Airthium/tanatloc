<%
    const getMeshOutput = (name) => {
        const output = ['"{"']
        output.push('"\\\"type\\\": \\\"COUPLING\\\", "')
        output.push('"\\\"name\\\": \\\"\" + ' + name + ' + \".mesh\\\""')
        output.push('"}"')

        return output.join('\n\t\t<< ')
    }

    const getDataOutput = (name) => {
        const output = ['"{"']
        output.push('"\\\"type\\\": \\\"COUPLING\\\", "')
        output.push('"\\\"name\\\": \\\"\" + ' + name + ' + \".dat\\\""')
        output.push('"}"')

        return output.join('\n\t\t<< ')
    }
-%>
// Data for coupling
appendLog("Save coupling data...");

savemesh(<%- mesh -%>, "<%- path -%>/" + <%- name -%> + ".mesh");
{
    ofstream coupling("<%- path -%>/" + <%- name -%> + ".dat");
    coupling << <%- data.join('[] << ') -%>[];
}

if (mpirank == 0) {
    ofstream processOut("process_data.log", append);
    processOut << <%- getMeshOutput(name) -%> << endl;
    processOut << <%- getDataOutput(name) -%> << endl;
}