	ofstream data("<%= path %>/" + <%- fileName %> + ".dat");
	data << "{" << endl;
	data << "  \"title\": \"" << "<%= title -%>" << "\"," << endl;
	data << "  \"names\": [" << endl;
<% dataNames.forEach((dataName, index) => { -%>
