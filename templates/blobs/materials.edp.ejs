// Materials
appendLog("Define materials...");

<% materials.children.forEach(material => { -%>
<% let firstValue = 0 -%>
// <%- material.label %> 
func <%= material.name -%> = 
<% materials.values.forEach((value, index) => { -%>
<% value.material.children.forEach(child => { -%>
<% if (child.symbol === material.name) { -%>
<% if (index === 0) { -%>
<% firstValue = child.value -%>
    <%= child.value %>
<% } else { -%>
    (<%= child.value -%> - <%= firstValue -%>)
<% value.selected.forEach(select => { -%>
            * (region == <%= select.label -%>)
<% }) -%>
<% } -%>
<% } -%>
<% }) -%>
<% }) -%>
    ;
<% }) -%>
