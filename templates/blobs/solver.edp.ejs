// Solver
<% if (
	solver.value === "UMFPACK"
	|| solver.value === "CG"
	|| solver.value === "Cholesky"
	|| solver.value === "Crout"
	|| solver.value === "GMRES"
	|| solver.value === "LU"
	|| solver.value === "sparsesolver") { -%>
func solver = <%= solver.value -%>;
<% } else if (solver.value === "MUMPS") { -%>
load "<%= solver.value -%>"
func solver = sparsesolver;
<% } else if (solver.value === "Crout") { -%>
func solver = "<%= solver.value -%>";
<%# Default solver -%>
<% } else { -%>
load "MUMPS"
func solver = sparsesolver;
<% } -%>
// End Solver